<odoo>
  <data>
           <!--Display form view of wizard-->    
        <record id="view_hr_attendance_excel_report_wizard_form" model="ir.ui.view">
            <field name="name">attendance.report.excel.wizard.form</field>
            <field name="model">attendance.report.excel.wizard</field>
            <field name="arch" type="xml">
                <form string="Attendance Report Excel">
                    <group>
                        
                       <group>
                              <field name="start_date" />
                      </group>
                        
                      <group>
                        <field name="end_date" />
                      </group>
                        
                    </group>
                    <notebook>
                        <page string="Employees">
                            <field name="employee_ids">
                                <tree editable="bottom">
                                    <field name="id" invisible="1"/>
                                    <field name="name" />
                                    <field name="department_id" /> 
                                    <field name="company_id" />                                    
                                </tree>
                            </field>
                        </page>
                    </notebook>
                    <footer>
                        <button name="action_gnerate_excel" type="object" string="Create Excel Report"
                           class="oe_highlight"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <!--Open the wizard on click of menu-->
        <record id="action_hr_attendance_report_wizard" model="ir.actions.act_window">
            <field name="name">Attendance Report Excel</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">attendance.report.excel.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="de_hr_attendance_report_xlsx.view_hr_attendance_excel_report_wizard_form"/>
        </record>
      
     
  </data>
</odoo>